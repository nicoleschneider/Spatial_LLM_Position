@article{Bertella2022,
  title={A systematic review of spatial approximations in spatial database systems},
  author={Bertella, Pedro Gabriel Kohl and Lopes, Yuri Kaszubowski and de Oliveira, Rafael Alves Paes and Carniel, Anderson Chaves},
  journal={Journal of Information and Data Management},
  volume={13},
  number={2},
  year={2022},
  groups={compass, spatial_pattern_match}, 
  abstract={},
  comment={Bertella et. al. conduct a survey of spatial approximation methods in 2022, expanding their 2021 review of the same topic with a more robust methodology. Their survey focuses on topological approximation, and does not consider hierarchical data structures (e.g. regions to locations to objects) and does not consider approximate spatial pattern matching. Their taxonomoy of query types \textit{topological, metric} and \textit{direction} are consistent with the broader literature. They highlight the importance of the "Approximate and Filter" approach to reduce complexity, which is consistent with the use of the Inverted Index in GESTALT to prune the candidate locations. }
}

@inproceedings{Carniel2020,
  title={Spatial information retrieval in digital ecosystems: A comprehensive survey},
  author={Carniel, Anderson Chaves},
  booktitle={Proceedings of the 12th International Conference on Management of Digital EcoSystems},
  pages={10--17},
  year={2020},
  groups={spatial_pattern_matching, compass},
  abstract={},
  comment={Carniel's 2020 survey of Spatial Information Retrieval aims to formalize a taxonomy of spatial query types. They identify Topological, Metric, and Direction as the relationships queries are built on, noting that the majority of queries seek to satisfy topological and metric criteria. They explain that direction relations can include (inter)cardinal and angular relations. While they note that Spatial Similarity queries are an area of interest, they do not identify any work explicitly, and do not discuss how cardinality invariant queries would work for direction based queries.}
}

@article{Carniel2023,
  title={Defining and designing spatial queries: the role of spatial relationships},
  author={Chaves Carniel, Anderson},
  journal={Geo-spatial Information Science},
  pages={1--25},
  year={2023},
  publisher={Taylor \& Francis},
  groups={compass, spatial_pattern_matching},
  abstract={},
  comment={In 2023, Carniel extends his earlier survey of spatial relations and queries to address the absence of hybrid spatial queries. The dicussion of angular and (inter)cardinal queries assumes that they are aligned to the global coordinate system. Of note to GESTALT, COMPASS abstracts away metric and topological features to only reason on directional relations. GESTALT's hierarchical data structure does conduct an implicit enclosure query - where we know that for a match to occur it must be enclosed in a location.}
}

@article{Chen2019,
  title={ESPM: Efficient spatial pattern matching},
  author={Chen, Hongmei and Fang, Yixiang and Zhang, Ying and Zhang, Wenjie and Wang, Lizhen},
  journal={IEEE Transactions on Knowledge and Data Engineering},
  volume={32},
  number={6},
  pages={1227--1233},
  year={2019},
  publisher={IEEE},
  groups={compass, spatial_pattern_matching},
  comment={ESPM is proposed by the same research group who introduced Spatial Pattern Matching as a query type. They extend their earlier work in Spatial Pattern Matching using IR-Trees into Subgraph Matchinng by replacing the IR-Tree with an Inverted Linear Quadtree, which performs more efficently when working on large search spaces. Their baseline datasets provide a useful reference point we should consider measuring against. ESPM takes $\mathcal{O}(|D_w|^n)$ time, where n = $|V|$ and $|D_w|$ = $max{|D_i||D_i is the set of objects with query keyword w_i}$.}
}

@article{Dellapenna2012,
  title={A spatial relation-based framework to perform visual information extraction},
  author={Della Penna, Giuseppe and Magazzeni, Daniele and Orefice, Sergio},
  journal={Knowledge and information systems},
  volume={30},
  pages={667--692},
  year={2012},
  publisher={Springer},
  groups={compass,spatial_pattern_matching},
  abstract={},
  comment={Della Penna et al seek to conduct information extraction of PDF documents so that they can query the content of the document based on its spatial relations (e.g. positions of images on a page). They develop the Spatial Relation Query Language (SRQL) that allows for direction relation queries to be specified in a SQL-like syntax. Their spatial concatenation queries allow for identificatons of objects that are LEFT, RIGHT, UP and DOWN from each other. The work itself is of limited direct utility because of the focus on PDF documents, but the ability to query by relative locations is useful. The SRQL query interface is much less intuitive to a lay-user than a pictorial interface is. This paper is relevant to GESTALT and COMPASS because it provides an example of another system that conducts spatial pattern matching based on the arrangement of objects.}
}

@article{Dellapenna2017,
  title={A formal framework to represent spatial knowledge},
  author={Della Penna, Giuseppe and Magazzeni, Daniele and Orefice, Sergio},
  journal={Knowledge and Information Systems},
  volume={51},
  pages={311--338},
  year={2017},
  publisher={Springer},
  groups={compass,spatial_pattern_matching},
  abstract={},
  comment={Della Penna et. al.'s 2017 work seeks to formally define spatial relations into a taxonomy. Their taxonomy is comprehensive, but the concepts of particular use to GESTALT and COMPASS are \textit{Concatenation} relations (effectively, those that arent intersection or containment relations). Within concatenation queries they identify the postitional \textit{Left, Right, Up, Down} as well as a \textit{weak} modifier. They also describe \textit{linked} relations, which resemble graphs or constraint satisfaction networks. The difference is that the linked strucutres explicitly encode the relationships between objects, where they are implicit in the other. While their language is expressive, it is not suitable for the tasks we use GESTALT for, as it focuses on information extraction tasks rather than navigation or geospatial search.}

}

@article{Du2017,
  title={An effective high-performance multiway spatial join algorithm with spark},
  author={Du, Zhenhong and Zhao, Xianwei and Ye, Xinyue and Zhou, Jingwei and Zhang, Feng and Liu, Renyi},
  journal={ISPRS International Journal of Geo-Information},
  volume={6},
  number={4},
  pages={96},
  year={2017},
  publisher={MDPI},
  groups={compass, spatial_pattern_matching},
  abstract={},
  comment={Du et al investigate how to use Apache Spark to paralellize multiway spatial joints and improve execution time. They seek to improve on the current optimization approaches which focused on algorithmic improvement in serial implementations, naive parallelization and map reduce as the existing approaches to leverage more computing power. Their method occurs over three phases. First, they partition the underlying space. Then they group the partitions together by objects and finally conduct a cascading pairwise spatial join. Their empirical evaluation suggests that selecting a discriminative join order is essential to efficent execution. Their system appears to only support topological and possibly metric queries, and so is not suitable for use in GESTALT/COMPASS which relies on directional relationships to prune the result space.}
}

@inproceedings{Fang2018,
  title={SpaceKey: exploring patterns in spatial databases},
  author={Fang, Yixiang and Cheng, Reynold and Wang, Jikun and Budiman, Lukito and Cong, Gao and Mamoulis, Nikos},
  booktitle={2018 IEEE 34th International Conference on Data Engineering (ICDE)},
  pages={1577--1580},
  year={2018},
  organization={IEEE}, 
  groups={compass, gestalt, spatial_pattern_matching, system}, 
  abstract={},
  comment={SpaceKey is a system designed for spatial pattern matching. It seeks to broaden spatial searching by offering features beyond what is commonly available with popular spatial keyword queries. SpaceKey allows users to search for patterns of objects based on metric (distance) relations and the inclusion or exclusion of certain objects within those distances. The detail of query completion is not discussed in the paper. the The system has a pictorial query interface, and while it appears to allow a user to query by the relative directions of objects, closer examination suggests that the directions of objects have no bearing on the search results. Overall, the system shows that spatial pattern matching is an area of active research in geospatial computing and that there is a gap in direction relation pattern matching.}
}


@article{Fang2019,
  title={Evaluating pattern matching queries for spatial databases},
  author={Fang, Yixiang and Li, Yun and Cheng, Reynold and Mamoulis, Nikos and Cong, Gao},
  journal={The VLDB Journal},
  volume={28},
  pages={649--673},
  year={2019},
  publisher={Springer},
  groups={compass, spatial_pattern_matching},
  abstract={},
  comment={Fang et al.'s evaluation paper sets out to review spatial pattern matching approaches and evaluate them. They focus their comparison between Spatial Keyword Matching, Multi-Way Spatial Joins and Spatial Pattern Matching (their approach). They define the SPM problem formally as a graph requiring that each vertex has a keyword label, each edge has a distance interval and each edge has a sign (exclusion in, exclusion out, mutual exclusion and multiple inclusion). COMPASS' approach violates all of these conditions by not formulating the SPM problem as a graph problem. However, we do require that objects have labels. They implement two algorithms to solve spatial pattern matching, both of which rely on approximate isomorphic subgraph matching to solve. Their \textbf{Multi-Pair-Join (MPJ)} algorithm runs in $\mathcal{O}(m\zeta |D|^2+\xi)$ where $\zeta$ is a sampling threshold in range $[0,1]$ and $\xi$ is the maximal number of partial matches. Their \textbf{Multi-Star-Join (MSJ)} algorithm runs in $\mathcal{O}(n^4+m|D|^2+\xi)$ time worst case, but practically its running time is faster than the MPJ. The SPM algorithm is extended with two sub-variants. Top-K SPM is used to refine the results set when recall is too high, and Partial SPM is used when too few results are returned. Both incur additional computational cost. Their evaluation is focused on execution time, and shows that both of their approaches are faster to execute over the spatial pattern matching problem than generic subgraph matching algorithms are.}
}

