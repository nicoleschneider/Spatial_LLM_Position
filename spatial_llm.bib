@article{Liu2003,
  title={Object-based directional query processing in spatial databases},
  author={Liu, Xuan and Shekhar, Shashi and Chawla, Sanjay},
  journal={IEEE transactions on knowledge and data engineering},
  volume={15},
  number={2},
  pages={295--304},
  year={2003},
  publisher={IEEE},
  groups={compass, spatial_pattern_matching, cardinality_invariant},
  abstract={},
  comment={Liu et. al. use an open shape-based strategy to search for relatively positioned objects not aligned to the global coordinate system. For example, being "In Front" of a building that is oriented "west", when the global coordinate system assumes "front" is "north". They build on earlier work using a Range Query Strategy (RQS) where the minimum (globally aligned) bounding rectangle that covers the query space is used for initial retrieval, and then pruned. OSS creates an open shape based on the query parameters and then uses containment and intersection queries to check for objects which satisfy the query. This work is closely aligned to COMPASS in that it assumes the query is from the perspective of the user, and it allows for querying based on the realive position of objects. However, it does not allow for directional pattern matching or keyword pattern matching, both of which are important to the functioning of COMPASS and GESTALT.}
}

